'.source.coursescript':
  'Presentation Video':
    'prefix': 'prevideo'
    'body': """
      [TYPE Pre]
      ID: $1
      Video: $2
      VideoClip: $3
      VideoClip: $4
    """
  'Presentation Audio':
    'prefix': 'preaudio'
    'body': """
      [TYPE Pre]
      ID: $1
      DF$2: $3
      Pic: $4
      Audio: $5
      Animation:
      Pic(effect=${6:fadeIn},time=$7): $8
    """
  'Presentation Reading':
    'prefix': 'prereading'
    'body': """
      [TYPE Pre]
      ID: $1
      Title: $2
      Pic: $3
      Text: $4
    """
  'Open Question':
    'prefix': 'openquestion'
    'body': """
      [TYPE OpenQuestion]
      ID: $1
      Pic: $2
      Audio: $3
    """
  'SentenceFragments':
    'prefix': 'sentf'
    'body': """
      [TYPE SentenceFragments]
      ID: $1
      Text: $2
      Text: $3
      TR_Text: $4
    """
  'SpotErrors':
    'prefix': 'spote'
    'body': """
      [TYPE SpotErrors]
      ID: $1
      Text: $2
      \"""
      tips:
      - activity:
        - $3
      \"""
    """
  'Locating':
    'prefix': 'locating'
    'body': """
      [TYPE Locating]
      ID: $1
      Audio: $2
    """
  'Cloze':
    'prefix': 'cloze'
    'body': """
      [TYPE Cloze]
      ID: $1
      Audio: $2
      Audio: $3
      \"""
      tips:
      - choice:
        - $4
        activity:
        - $5
      - choice:
        - $6
        activity:
        - $7
      \"""
    """
  'Dictation':
    'prefix': 'dictation'
    'body': """
      [TYPE Dictation]
      ID: $1
      // example: Scientists tell us that {{what we see in the universe}} is only a small part of what is there.
      Audio: $2
    """
  'MCQ5':
    'prefix': 'mcq5'
    'body': """
      [TYPE MCQ5]
      ID: $1
      Audio: $2
      Pic: $3
      \"""
      tips:
      - stem:
        - $4
        choice:
        - $5
      \"""
    """
  'MCQ5a':
    'prefix': 'mcq5a'
    'body': """
      [TYPE MCQ5a]
      ID: $1
      Audio(question=true): $2
      Audio(matching=true): $3
      \"""
      tips:
      - stem:
        - $4
        choice:
        - $5
      \"""
    """
  'Sequencing':
    'prefix': 'seq'
    'body': """
      [TYPE Sequencing]
      ID: $1
      Audio: $2
      Audio: $3
      Audio: $4
      \"""
      tips:
      - activity:
        - $5
      \"""
    """
  'RP':
    'prefix': 'rp'
    'body': """
      [TYPE RP]
      ID: $1
      Pic(role=$1): $2
      Pic(role=$2): $3
      Audio(role=$1): $4
      Audio(role=$2): $5
      Audio(role=$1): $6
      Audio(role=$2): $7
      Audio(role=$1): $8
    """
  'OR':
    'prefix': 'or'
    'body': """
      [TYPE OR]
      ID: $1
      Pic: $2
      Audio: $3
      TR: $4
    """
  'SR':
    'prefix': 'sr'
    'body': """
      [TYPE SR]
      ID: $1
      Pic: $2
      Audio: $3
      TR: $4
    """
  'C&D':
    'prefix': 'cnd'
    'body': """
      [TYPE C&D]
      ID: $1
      Pic: $2
      Text: $3
      \"""
      tips:
      - activity:
        - $4
      - activity:
        - $5
      \"""
    """
  'MCQ6':
    'prefix': 'mcq6'
    'body': """
      [TYPE MCQ6]
      ID: $1
      Audio: $2
      Pic: $3
      Opts:
        Audio: $4
        Audio(checked=true): $5
        Audio: $6
      TR: $7
      \"""
      tips:
      - stem:
        - $8
        activity:
        - $9
      \"""
    """
  'MCQ1':
    'prefix': 'mcq1'
    'body': """
      [TYPE MCQ1]
      ID: $1
      Audio: $2
      Pic: $3
      Text: $4
      Opts:
        Text: $5
        Text(checked=true): $6
        Text: $7
        Text: $8
      TR: $9
      \"""
      tips:
      - stem:
        - $10
        activity:
        - $11
      \"""
    """
  'MCQ2':
    'prefix': 'mcq2'
    'body': """
      [TYPE MCQ2]
      ID: $1
      Audio: $2
      Pic: $3
      Text: $4
      Opts:
        Text: $5
        Text(checked=true): $6
        Text: $7
        Text: $8
      TR: $9
      \"""
      tips:
      - stem:
        - $10
        choice:
        - $11
        activity:
        - $12
      \"""
    """
  'MCQ3':
    'prefix': 'mcq3'
    'body': """
      [TYPE MCQ3]
      ID: $1
      Audio: $2
      Pic: $3
      Text: $4
      Opts:
        Text: $5
        Text(checked=true): $6
        Text: $7
        Text: $8
      TR: $9
    """
  'MCQ1a':
    'prefix': 'mcq1a'
