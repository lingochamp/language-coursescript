  'MCP3':
    'prefix': 'mcp3'
    'body': """
      [TYPE MCP3]
      ID: $1
      Audio: $2
      Opts:
        Text: $3
        Text: $4
        Pic(checked=true): $5
      TR: $6
    """
  'WordGuess':
    'prefix': 'wordguess'
    'body': """
      [TYPE WordGuess]
      ID: $1
      Audio: $2
      Text: $3
      TR: $4
    """
  'ChooseAllWords':
    'prefix': 'chooseall'
    'body': """
      [TYPE ChooseAllWords]
      ID: $1
      Opts:
        Text(checked=true): $2
        Text(checked=true): $3
        Text: $4
        Text: $5
        Text: $6
        Text: $7
    """
  'WordFragments':
    'prefix': 'wordfragments'
    'body': """
      [TYPE WordFragments]
      ID: $1
      Pic: $2
      Audio: $3
      Text: $4
    """
  'SentenceCompletion':
    'prefix': 'sentencecompletion'
    'body':"""
      [TYPE SentenceCompletion]
      ID: $1
      Pic: $2
      Audio: $3
      Text: $4
    """
  'Level':
    'prefix': "level"
    'body': """
    = Level $1
    """
  'Milestone':
    'prefix': "milestone"
    'body': """
    == Milestone $1
    \"""
    summary: $2
    \"""
    """
  'Session':
    'prefix': "session"
    'body': """
    === Session $1 $2
    \"""
    name: $3
    \"""
    """
  'Episode':
    'prefix': "episode"
    'body': """
    ==== Episode $1
    \"""
    intro: $2
    intro_language:
      zh-Hans: $3
    \"""
    """
  'Group':
    'prefix': "group"
    'body': """
    ===== Group $1
    """
