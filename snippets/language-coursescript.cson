      [TYPE MCQ1a]
      ID: $1
      Audio: $2
      Pic: $3
      Text: $4
      Opts:
        Text: $5
        Text(checked=true): $6
        Text: $7
        Text: $8
      TR: $9
    """
  'MCQ2a':
    'prefix': 'mcq2a'
    'body': """
      [TYPE MCQ2a]
      ID: $1
      Audio: $2
      Pic: $3
      Text: $4
      Opts:
        Text: $5
        Text(checked=true): $6
        Text: $7
        Text: $8
      TR: $9
    """
  'MCQ4a':
    'prefix': 'mcq4a'
    'body': """
      [TYPE MCQ4a]
      ID: $1
      Audio: $2
      Pic: $3
      Text: $4
      Opts:
        Text: $5
        Text(checked=true): $6
        Text: $7
        Text: $8
      TR: $8
    """
  'MCQ7':
    'prefix': 'mcq7'
    'body': """
      [TYPE MCQ7]
      ID: $1
      Audio: $2
      Pic: $3
      Text: $4
      Opts:
        Text: $5
        Text(checked=true): $6
        Text: $7
        Text: $8
      TR: $9
    """
  'MCP1':
    'prefix': 'mcp1'
    'body': """
      [TYPE MCP1]
      ID: $1
      Audio: $2
      Opts:
        Text: $3
        Text: $4
        Pic(checked=true): $5
      TR: $6
      \"""
      tips:
      - stem:
        - $7
      \"""
    """
  'MCP2':
    'prefix': 'mcp2'
    'body': """
      [TYPE MCP2]
      ID: $1
      Audio: $2
      Opts:
        Text: $3
        Text: $4
        Pic(checked=true): $5
      TR: $6
      \"""
      tips:
      - stem:
        - $7
      \"""
